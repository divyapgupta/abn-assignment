{
  "name": "abn-amro-frontend-assignment",
  "version": "1.0.0",
  "description": "ABN AMRO Frontend developer assignment",
  "homepage": "https://github.com/divyapgupta/abn-assignment#readme",
  "bugs": {
    "url": "https://github.com/divyapgupta/abn-assignment/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/divyapgupta/abn-assignment.git"
  },
  "license": "ISC",
  "author": "Divya Prakash Gupta",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "npm run type-check && vite build",
    "dev": "vite",
    "format": "npm run lint:fix && npm run prettier:fix",
    "lint": "eslint './**/*.{ts,vue,js}'",
    "lint:fix": "eslint './**/*.{ts,vue,js}' --fix",
    "prepare": "husky",
    "prettier": "prettier ./ --check",
    "prettier:fix": "prettier ./ --write",
    "preview": "npm run build && vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "type-check": "vue-tsc"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,html,css}": [
      "npm run format"
    ],
    "*.{ts,tsx,vue}": []
  },
  "dependencies": {
    "vue": "^3.5.28",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^10.0.1",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/vue": "^8.1.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.2.3",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.8.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jsdom": "^28.1.0",
    "lint-staged": "^16.2.7",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue-test-utils": "^0.5.0",
    "vue-tsc": "^3.2.4"
  }
}