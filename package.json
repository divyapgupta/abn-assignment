{
  "name": "abn-amro-frontend-assignment",
  "version": "1.0.0",
  "description": "ABN AMRO Frontend developer assignment",
  "homepage": "https://github.com/divyapgupta/abn-assignment#readme",
  "bugs": {
    "url": "https://github.com/divyapgupta/abn-assignment/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/divyapgupta/abn-assignment.git"
  },
  "license": "ISC",
  "author": "Divya Prakash Gupta",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "npm run type-check && vite build",
    "dev": "vite",
    "format": "npm run lint:fix && npm run prettier:fix",
    "lint": "eslint './**/*.{ts,vue,js}'",
    "lint:fix": "eslint './**/*.{ts,vue,js}' --fix",
    "prepare": "husky",
    "prettier": "prettier ./ --check",
    "prettier:fix": "prettier ./ --write",
    "preview": "npm run build && vite preview",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "vue-tsc"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,html,css}": [
      "npm run format"
    ],
    "*.{ts,tsx,vue}": [
      "npm run type-check"
    ]
  },
  "dependencies": {
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@eslint/css": "^0.9.0",
    "@eslint/js": "^9.29.0",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^24.0.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.2.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.10"
  }
}
